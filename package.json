{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "thinsim",
	"version": "0.1.0",
	"homepage": "https://thinsim.risunosu.com",
	"author": "Risu (risu729)",
	"license": "MIT",
	"private": true,
	"scripts": {
		"commit": "git-cz",
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"test": "vitest --ui",
		"coverage": "vitest run",
		"lint": "npm-run-all --continue-on-error lint:*",
		"format": "npm-run-all format:*",
		"ignore-sync": "ignore-sync .",
		"lint:prettier": "prettier --check .",
		"lint:eslint": "eslint --max-warnings=0 .",
		"lint:tsc": "tsc --noEmit --incremental false",
		"lint:markdown": "markdownlint-cli2 \"**/*.md\"",
		"lint:cspell": "cspell --relative --no-must-find-files \"**/*\"",
		"lint:secret": "secretlint --maskSecrets \"**/*\"",
		"lint:knip": "knip",
		"format:prettier": "prettier --write .",
		"format:eslint": "eslint --fix --max-warnings=0 .",
		"prepare": "husky install"
	},
	"type": "module",
	"volta": {
		"node": "20.7.0"
	},
	"packageManager": "pnpm@8.7.6",
	"dependencies": {
		"@chakra-ui/next-js": "2.1.5",
		"@chakra-ui/react": "2.8.1",
		"@emotion/react": "11.11.1",
		"@emotion/styled": "11.11.0",
		"framer-motion": "10.16.4",
		"jotai": "2.4.2",
		"konva": "9.2.1",
		"next": "13.4.19",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-konva": "18.2.10",
		"react-use": "17.4.0"
	},
	"devDependencies": {
		"@commitlint/cli": "17.7.1",
		"@commitlint/config-conventional": "17.7.0",
		"@commitlint/cz-commitlint": "17.7.1",
		"@commitlint/types": "17.4.4",
		"@cspell/cspell-types": "7.3.6",
		"@eslint/js": "8.50.0",
		"@humanwhocodes/gitignore-to-minimatch": "1.0.2",
		"@next/eslint-plugin-next": "13.4.19",
		"@secretlint/secretlint-rule-preset-recommend": "7.0.7",
		"@secretlint/types": "7.0.7",
		"@testing-library/jest-dom": "6.1.3",
		"@testing-library/react": "14.0.0",
		"@testing-library/user-event": "14.5.1",
		"@tsconfig/next": "2.0.0",
		"@types/micromatch": "4.0.2",
		"@types/node": "20.6.3",
		"@types/react": "18.2.22",
		"@types/react-dom": "18.2.7",
		"@typescript-eslint/eslint-plugin": "6.7.2",
		"@typescript-eslint/parser": "6.7.2",
		"@vitejs/plugin-react": "4.1.0",
		"@vitest/coverage-v8": "0.34.5",
		"@vitest/ui": "0.34.5",
		"commitizen": "4.3.0",
		"cspell": "7.3.6",
		"eslint": "8.50.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-config-vitest-globals": "0.0.3",
		"eslint-gitignore": "0.1.0",
		"eslint-plugin-jsx-a11y": "6.7.1",
		"eslint-plugin-react": "7.33.2",
		"eslint-plugin-react-hooks": "4.6.0",
		"eslint-plugin-unicorn": "48.0.1",
		"eslint-plugin-vitest": "0.3.1",
		"eslint-plugin-yml": "1.9.0",
		"globals": "13.22.0",
		"husky": "8.0.3",
		"ignore-sync": "7.0.1",
		"jsdom": "22.1.0",
		"knip": "2.27.1",
		"lint-staged": "14.0.1",
		"markdownlint-cli2": "0.10.0",
		"micromatch": "4.0.5",
		"npm-run-all": "4.1.5",
		"prettier": "3.0.3",
		"prettier-plugin-organize-imports": "3.2.3",
		"secretlint": "7.0.7",
		"typescript": "5.2.2",
		"vite-tsconfig-paths": "4.2.1",
		"vitest": "0.34.5",
		"yaml-eslint-parser": "1.2.2"
	}
}
