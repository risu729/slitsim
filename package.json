{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "thinsim",
	"version": "0.1.0",
	"homepage": "https://thinsim.risunosu.com",
	"author": "Risu (risu729)",
	"license": "MIT",
	"private": true,
	"scripts": {
		"commit": "git-cz",
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"test": "vitest --ui",
		"coverage": "vitest run",
		"lint": "npm-run-all --continue-on-error lint:*",
		"format": "npm-run-all format:*",
		"ignore-sync": "ignore-sync .",
		"lint:prettier": "prettier --check .",
		"lint:eslint": "eslint --max-warnings=0 .",
		"lint:tsc": "tsc --noEmit --incremental false",
		"lint:markdown": "markdownlint-cli2 \"**/*.md\"",
		"lint:cspell": "cspell --relative --no-must-find-files \"**/*\"",
		"lint:secret": "secretlint --maskSecrets \"**/*\"",
		"lint:knip": "knip",
		"format:prettier": "prettier --write .",
		"format:eslint": "eslint --fix --max-warnings=0 .",
		"prepare": "husky install"
	},
	"type": "module",
	"volta": {
		"node": "21.4.0",
		"pnpm": "8.12.0"
	},
	"dependencies": {
		"@chakra-ui/next-js": "2.2.0",
		"@chakra-ui/react": "2.8.2",
		"@emotion/react": "11.11.1",
		"@emotion/styled": "11.11.0",
		"framer-motion": "10.16.16",
		"jotai": "2.6.0",
		"konva": "9.2.3",
		"next": "13.4.19",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-konva": "18.2.10",
		"react-use": "17.4.2"
	},
	"devDependencies": {
		"@commitlint/cli": "18.4.3",
		"@commitlint/config-conventional": "18.4.3",
		"@commitlint/cz-commitlint": "18.4.3",
		"@commitlint/types": "18.4.3",
		"@cspell/cspell-types": "8.1.3",
		"@eslint/js": "8.55.0",
		"@humanwhocodes/gitignore-to-minimatch": "1.0.2",
		"@next/eslint-plugin-next": "13.4.19",
		"@secretlint/secretlint-rule-preset-recommend": "8.0.0",
		"@secretlint/types": "8.0.0",
		"@testing-library/jest-dom": "6.1.5",
		"@testing-library/react": "14.1.2",
		"@testing-library/user-event": "14.5.1",
		"@tsconfig/next": "2.0.1",
		"@types/micromatch": "4.0.6",
		"@types/node": "20.10.4",
		"@types/react": "18.2.45",
		"@types/react-dom": "18.2.17",
		"@typescript-eslint/eslint-plugin": "6.14.0",
		"@typescript-eslint/parser": "6.14.0",
		"@vitejs/plugin-react": "4.2.1",
		"@vitest/coverage-v8": "1.0.4",
		"@vitest/ui": "1.0.4",
		"commitizen": "4.3.0",
		"cspell": "8.1.3",
		"eslint": "8.55.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-config-vitest-globals": "0.0.3",
		"eslint-gitignore": "0.1.0",
		"eslint-plugin-jsx-a11y": "6.8.0",
		"eslint-plugin-react": "7.33.2",
		"eslint-plugin-react-hooks": "4.6.0",
		"eslint-plugin-unicorn": "49.0.0",
		"eslint-plugin-vitest": "0.3.16",
		"eslint-plugin-yml": "1.10.0",
		"globals": "13.24.0",
		"husky": "8.0.3",
		"ignore-sync": "7.0.1",
		"jsdom": "23.0.1",
		"knip": "3.7.1",
		"lint-staged": "15.2.0",
		"markdownlint-cli2": "0.11.0",
		"micromatch": "4.0.5",
		"npm-run-all": "4.1.5",
		"prettier": "3.1.1",
		"prettier-plugin-organize-imports": "3.2.4",
		"secretlint": "8.0.0",
		"typescript": "5.3.3",
		"vite-tsconfig-paths": "4.2.2",
		"vitest": "1.0.4",
		"yaml-eslint-parser": "1.2.2"
	}
}
